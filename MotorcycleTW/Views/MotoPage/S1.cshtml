@section topCSS {
<style>
    .gs-product-nav {
        position: relative;
        z-index: 1000;
        font-size: 14px;
    }
    .gs-product-nav-inner-wrap.sticking {
        position: fixed;
        z-index: 1000;
        left: 0;
        top:0;
        width: 100%;
        max-width: 100%;
        background:#000000;
        height:44px;
    }
    .gs-product-nav__curtain {
        display: none;
        position: fixed;
        left: 0;
        top: 0;
        bottom: 0;
        right: 0;
    }
    .gs-product-nav__list {
        display: block;
        position: absolute;
        left: 0;
        width: 100%;
        top: 43px;
        max-height: 0;
        overflow: hidden;
        text-align: center;
        transition: max-height 0.5s, background 0.5s;
    }
    .gs-product-nav__item{
        padding:0 10px;
    }

    .reset-list {
        list-style: none;
        padding: 0;
        margin: 0;
